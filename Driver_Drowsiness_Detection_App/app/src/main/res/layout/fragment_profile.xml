<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#201A31"
    tools:context=".ProfileFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <TextView
        android:id="@+id/profile_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:textColor="@color/white"
        android:textSize="26sp"
        android:fontFamily="@font/montserrat_bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        />

    <TextView
        android:id="@+id/first_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/profile_title"
        android:textColor="@color/white"
        android:layout_marginStart="55dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="69dp"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/first_name_edt_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_edt"
        android:hint="First Name"
        android:drawableLeft="@drawable/ic_baseline_phone_24"
        android:textColorHint="#ffffff"
        android:paddingTop="16dp"
        android:paddingStart="25dp"
        android:paddingBottom="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/first_name_text"
        android:layout_marginTop="6dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="38dp"/>

    <TextView
        android:id="@+id/last_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/first_name_edt_profile"
        android:textColor="@color/white"
        android:layout_marginStart="55dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="69dp"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/last_name_edt_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_edt"
        android:hint="Last name"
        android:drawableLeft="@drawable/ic_baseline_phone_24"
        android:textColorHint="#ffffff"
        android:paddingTop="16dp"
        android:paddingStart="25dp"
        android:paddingBottom="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/last_name_text"
        android:layout_marginTop="6dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="38dp"/>

    <TextView
        android:id="@+id/contact_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mobile Number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/last_name_edt_profile"
        android:textColor="@color/white"
        android:layout_marginStart="55dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="69dp"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/contact_edt_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_edt"
        android:hint="Mobile Number"
        android:drawableLeft="@drawable/ic_baseline_phone_24"
        android:textColorHint="#ffffff"
        android:paddingTop="16dp"
        android:paddingStart="25dp"
        android:paddingBottom="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/contact_text"
        android:layout_marginTop="6dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="38dp"/>

    <TextView
        android:id="@+id/emergency_contact_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emergency Contact"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/contact_edt_profile"
        android:textColor="@color/white"
        android:layout_marginStart="55dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="69dp"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/emergency_contact_edt_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_edt"
        android:hint="Emergency Contact"
        android:drawableLeft="@drawable/ic_baseline_phone_24"
        android:textColorHint="#ffffff"
        android:paddingTop="16dp"
        android:paddingStart="25dp"
        android:paddingBottom="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/emergency_contact_text"
        android:layout_marginTop="6dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="38dp"/>

    <TextView
        android:id="@+id/vehicle_number_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vehicle Number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/emergency_contact_edt_profile"
        android:textColor="@color/white"
        android:layout_marginStart="55dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="69dp"
        android:textSize="16sp"
        />

    <EditText
        android:id="@+id/vehicle_number_edt_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_edt"
        android:hint="Vehicle Number"
        android:drawableLeft="@drawable/ic_baseline_phone_24"
        android:textColorHint="#ffffff"
        android:paddingTop="16dp"
        android:paddingStart="25dp"
        android:paddingBottom="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/vehicle_number_text"
        android:layout_marginTop="6dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="38dp"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>